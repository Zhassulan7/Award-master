@model Awards.Models.Award
@{
    ViewData["Title"] = "AddAward";
}

<h1>AddAward</h1>

<form asp-action="AddAward">
    <div class="form-group">
        <label asp-for="Title">Name</label>
        <input asp-for="Title" type="text" class="form-control" placeholder="Введите название награды">
    </div>
    <div class="form-group">
        <label asp-for="Description">Password</label>
        <input asp-for="Description" type="text" class="form-control" placeholder="Описание награды">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>